   variable: exists_obs$obs_index {
     type: discrete observed $exists_index:$exists_index cardinality CARD_EXISTS;

     conditionalparents: nil using DenseCPT("internal:UnityScore");
   }

   variable: obs$obs_index {
     type: continuous observed $obs_index:$obs_index;

     switchingparents: exists_obs$obs_index(0) using mapping("map_parent");

     conditionalparents: nil using mixture("internal:UnityScore")
                         | seg(0) using mixture collection("collection_seg_obs$obs_index")
                           mapping("map_parent");
   }
